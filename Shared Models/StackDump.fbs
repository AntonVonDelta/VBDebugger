namespace NetModels;
file_identifier "DEBB";

table Variable{
	name:string;
	value:string;
}

table SourceCodeReference{
	filename:string;
	scope_name:string;
	line_number:int;
}

table StackFrame{
	reference: SourceCodeReference;
	locals:[Variable];
}

table StackDump{
	frames: [StackFrame];
	curent_instruction: SourceCodeReference;
	messages: [string];
}

root_type StackDump;