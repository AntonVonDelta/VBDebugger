namespace NetModels;

enum EventType : int {None, EnteredProcedure, LeaveProcedure, Log}

table DebugEvent{
	event_type : EventType;
	
	// Identifier for the current scope of the event
	scope_name: string (required);
	
	// The name of the source code
	source_file_name: string (required);
	
	// Line number where the event occured
	line_number: int;
	
	// Scope data
	scope_data: string (required);
}

root_type DebugEvent;